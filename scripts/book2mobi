from librarian.book2anything import Book2Anything


class Book2Mobi(Book2Anything):
    format_name = "MOBI"
    ext = "mobi"
    uses_cover = True
    cover_optional = False
    uses_provider = True


if __name__ == '__main__':
    Book2Mobi.run()
